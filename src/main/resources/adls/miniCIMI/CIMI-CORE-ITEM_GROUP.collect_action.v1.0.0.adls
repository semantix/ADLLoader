        value matches {
          TEXT[id0.0.6]
        }
      }
    }
    link matches {
      LINK[id0.0.4] occurrences matches {0..1} -- Collection procedure
    }
    participation matches {
      PARTICIPATION[id0.109.1] -- Collector
      PARTICIPATION[id0.109.2] occurrences matches {0..1} -- Locationof subject
      PARTICIPATION[id0.109.3] occurrences matches {0..1} -- Locationof collector
    }
  }

