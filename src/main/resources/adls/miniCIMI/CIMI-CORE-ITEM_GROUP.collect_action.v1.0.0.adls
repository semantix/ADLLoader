      ELEMENT[id2.6.1] -- Collection timing offset
      ELEMENT[id2.7.1] -- Collection timing offset origin
      ELEMENT[id0.0.1] matches { -- Collection description
        value matches {
          TEXT[id0.0.5]
        }
      }
      use_archetype ITEM_GROUP [id0.0.2, CIMI-CORE-ITEM_GROUP.specimen_collection_site.v1] -- Collection body site
